<!doctype html>
<html lang="no" class="nbeat-project">
  <head prefix="og: http://ogp.me/ns#">
    <title>{{pkg.display}}</title>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="https://www.northernbeat.no/nbeat-project.min.css" />
  </head>

  <body>
    <div class="bd-pageheader">
      <div class="container">
        <h1>{{pkg.display}}</h1>

        <p class="lead">
          {{pkg.description}}
        </p>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="col-12">
          {% block content %}
          Content
          {% endblock %}
        </div>
      </div>
    </div>
    
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="https://www.northernbeat.no/nbeat-project.js"></script>

  </body>
</html>
